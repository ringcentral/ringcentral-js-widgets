{
  "name": "ringcentral-js-widget",
  "version": "0.1.0",
  "scripts": {
    "test": "node node_modules/mocha/bin/mocha --compilers js:babel-core/register --require test/setup.js ./src/**/*.spec.js",
    "coverage": "node node_modules/.bin/istanbul cover node_modules/mocha/bin/_mocha -- --compilers js:babel-core/register --require ignore-styles --require test/setup.js --colors --reporter dot ./src/**/*.spec.js",
    "coverall": "node node_modules/.bin/istanbul cover node_modules/mocha/bin/_mocha --report lcovonly -- --compilers js:babel-core/register --require ignore-styles --require test/setup.js --colors --reporter dot ./src/**/*.spec.js && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "eslint": "node node_modules/eslint/bin/eslint.js src/*",
    "build": "node_modules/.bin/babel src --out-dir build --copy-files",
    "dist": "node node_modules/webpack/bin/webpack.js",
    "watch": "node node_modules/webpack/bin/webpack.js --watch",
    "showcase": "node ./node_modules/babel-cli/bin/babel-node.js  cli/dependency/index.js",
    "theme": "node ./node_modules/babel-cli/bin/babel-node.js  cli/theme/index.js",
    "test-data": "node ./node_modules/babel-cli/bin/babel-node.js  cli/test/index.js",
    "release": "npm run build && npm run theme && node ./node_modules/babel-cli/bin/babel-node.js  cli/release/index.js"
  },
  "main": "build/widgets/components.js",
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.4",
