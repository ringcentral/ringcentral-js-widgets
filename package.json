    "country-data": "0.0.25",
    "decamelize": "^1.2.0",
    "google-libphonenumber": "^1.0.27",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "ringcentral-js-integration-commons": "https://github.com/changbenny/ringcentral-js-integration-commons.git#add-dist",
    "ringcentral-web-phone": "^0.3.1"
